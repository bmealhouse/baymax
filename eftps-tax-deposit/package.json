{
  "private": true,
  "name": "eftps-tax-deposit",
  "version": "1.0.0",
  "description": "Schedules EFTPS tax deposits",
  "author": "Brent Mealhouse <bmealhouse@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "prettier": {
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false
  },
  "xo": {
    "prettier": true,
    "rules": {
      "capitalized-comments": "off",
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_"
        }
      ]
    }
  },
  "dependencies": {
    "clipboardy": "2.1.0",
    "esm": "3.2.25",
    "inquirer": "7.0.1",
    "puppeteer": "2.0.0"
  },
  "devDependencies": {
    "dotenv": "8.2.0",
    "prettier": "1.19.1",
    "xo": "0.25.3"
  },
  "scripts": {
    "start": "node index.js"
  }
}
